{
  "name": "save-save",
  "version": "0.0.1",
  "description": "This is save&save project",
  "keywords": [
    "static",
    "website",
    "save",
    "environment"
  ],
  "license": "",
  "author": "Cristea Andrei && Diacenco Serghei",
  "scripts": {
    "build:local": "NODE_ENV=development-local parcel build index.html",
    "dev:local": "NODE_ENV=development-local parcel index.html --open",
    "host:local": "ttab \"cd ./api && npm run start\"",
    "start:local": "run-s build:local host:local dev:local",
    "build": "NODE_ENV=production parcel build index.html",
    "serve": "NODE_ENV=development parcel build index.html",
    "dev": "NODE_ENV=development parcel index.html --open",
    "start": "npm run serve && npm run dev",
    "test": "jshint || true",
    "lint": "npm run lint:css && npm run lint:html && npm run lint:js",
    "lint:css": "stylelint \"./css/**/*.css\" --formatter verbose --fix",
    "lint:html": "npx htmlhint \"./*.html\"",
    "lint:js": "eslint \"**/*.ts\" --fix",
    "firebase-frontend:local": "npm run build && firebase emulators:start --only hosting",
    "firebase-backend:local": "firebase emulators:start --only functions",
    "tab:firebase-frontend:local": "ttab npm run firebase-frontend:local",
    "tab:firebase-backend:local": "ttab npm run firebase-backend:local",
    "firebase:local": "run-s tab:firebase-frontend:local tab:firebase-backend:local",
    "firebase:deploy": "npm run build && firebase deploy"
  },
  "devDependencies": {
    "@parcel/optimizer-cssnano": "^2.0.0-nightly.616",
    "@parcel/optimizer-htmlnano": "^2.0.0-nightly.612",
    "@parcel/packager-css": "^2.0.0-nightly.616",
    "@parcel/packager-html": "^2.0.0-nightly.612",
    "@parcel/packager-raw-url": "^2.0.0-nightly.2234",
    "@parcel/transformer-css": "^2.0.0-nightly.612",
    "@parcel/transformer-html": "^2.0.0-nightly.612",
    "@parcel/transformer-postcss": "^2.0.0-nightly.612",
    "@parcel/transformer-posthtml": "^2.0.0-nightly.612",
    "@parcel/transformer-webmanifest": "^2.0.0-nightly.2234",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-watch": "^7.0.0",
    "htmlhint": "^0.14.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-beta.1",
    "parcel-bundler": "^1.12.4",
    "postcss": "^8.2.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "stylelint": "^13.11.0",
    "stylelint-config-standard": "^20.0.0",
    "ttab": "^0.7.0",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "lodash": "^4.17.21",
    "npm": "^7.6.0",
    "pdfmake": "^0.1.70",
    "postcss-modules": "^4.0.0",
    "print-js": "^1.6.0",
    "semver": "^7.3.4",
    "ts-serializer": "^2.2.6",
    "uuid": "^3.4.0",
    "validatorjs": "^3.22.1"
  }
}
