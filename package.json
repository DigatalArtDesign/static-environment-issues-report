    "lint:css": "stylelint \"./css/**/*.css\" --formatter verbose --fix",
    "lint:html": "npx htmlhint \"./*.html\"",
    "lint:js": "eslint \"**/*.ts\" --fix"
  },
  "devDependencies": {
    "@parcel/optimizer-cssnano": "^2.0.0-nightly.612",
    "@parcel/optimizer-htmlnano": "^2.0.0-nightly.612",
    "@parcel/packager-css": "^2.0.0-nightly.612",
    "@parcel/packager-html": "^2.0.0-nightly.612",
    "@parcel/packager-raw-url": "^2.0.0-nightly.2234",
    "@parcel/transformer-css": "^2.0.0-nightly.612",
    "@parcel/transformer-html": "^2.0.0-nightly.612",
    "@parcel/transformer-postcss": "^2.0.0-nightly.612",
    "@parcel/transformer-posthtml": "^2.0.0-nightly.612",
    "@parcel/transformer-webmanifest": "^2.0.0-nightly.2234",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-watch": "^7.0.0",
    "htmlhint": "^0.14.2",
    "parcel": "^2.0.0-beta.1",
    "parcel-bundler": "^1.12.4",
    "postcss": "^8.2.6",
    "stylelint": "^13.11.0",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "lodash": "^4.17.21",
    "npm": "^7.6.0",
    "pdfmake": "^0.1.70",
    "semver": "^7.3.4",
    "ts-serializer": "^2.2.6",
    "uuid": "^3.4.0"
  }
